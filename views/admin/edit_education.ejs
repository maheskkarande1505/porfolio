<%- include("navbar.ejs") %>
<form action="/admin/update_education" method="post" enctype="multipart/form-data">
    <div class="container bg-dark text-white mt-5">
        <h1>Edit Education</h1>
        <div class="row">
            <div class="col-md-6 mb-4">
                <input type="hidden" name="qualification_id" value="<%-qualification.qualification_id%>">
                <label class="label">Enter Qualification</label>
                <input type="text" class="form-control" name="qualification_name" value="<%-qualification.qualification_name%>" required>
            </div>
            <div class="col-md-6 mb-4">
                <label class="label">Enter University</label>
                <input type="text" class="form-control" name="university" value="<%-qualification.university%>" required>
            </div>
            <div class="col-md-6 mb-4">
                <label class="label">Enter Passing Year</label>
                <input type="number" class="form-control" name="passing_year" value="<%-qualification.passing_year%>">
            </div>
            <div class="col-md-6 mb-4">
                <label class="label">Enter Percentage</label>
                <input type="number" class="form-control" name="percentage" value="<%-qualification.percentage%>">
            </div>
            <div class="col-md-12 mb-4 text-center">
                <button class="btn btn-lg btn-primary">Update</button>
            </div>
        </div>
    </div>
</form>
